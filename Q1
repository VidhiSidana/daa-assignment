#include<iostream>
using namespace std;
int binarysearch(int arr[],int target,int n){
    int first=0;
    int last=n-1;
    while(first<=last){

    
    int mid=(first+last)/2;
    if(arr[mid]==target)return mid;
    else if(target>arr[mid]){
        first=mid+1;
    }
    else{
        last=mid-1;
    }
   
}
    return -1;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<=n-1;i++){
        cin>>arr[i];
    }
    int k;
    cout<<"enter target";
    cin>>k;
   int t=binarysearch(arr, k, n);
   cout<<t;

    return 0;
}
